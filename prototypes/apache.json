[
	{
		"id": "apache.get_stub_status",
		"name": "Get Apache HTTP status",
		"delay": "1m",
		"zbxVer": "3.4",
		"history": "1d",
		"trends": "0",
		"group": "Apache",
		"alarmObject": "Apache",
		"valueType": "TEXT",
		"triggers": [{
			"prototype": "trigger.http_get",
			"name": "Failed to fetch Apache stub status page",
			"url": "https://httpd.apache.org/docs/current/mod/mod_status.html"
		}]
	},
	{
		"id": "apache.bytes_per_second",
		"key": "apache.bytes_per_second",
		"name": "Bytes per second",
		"type": "DEPENDENT_ITEM",
		"masterItem": "apache.get_stub_status",
		"group": "Apache",
		"alarmObject": "Apache",
		"units": "Bps",
		"valueType": "FLOAT",
		"preprocessing": [
			{
				"type": "REGEX",
				"params": "BytesPerSec:\\s+([0-9\\.]+)\r\n\\1"
			}
		]
	},
	{
		"id": "apache.conns_async_closing",
		"key": "apache.conns_async_closing",
		"name": "Connections async closing",
		"type": "DEPENDENT_ITEM",
		"masterItem": "apache.get_stub_status",
		"group": "Apache",
		"alarmObject": "Apache",
		"preprocessing": [
			{
				"type": "REGEX",
				"params": "ConnsAsyncClosing:\\s+([0-9]+)\r\n\\1"
			}
		],
		"graphs": [
			{
				"name": "Current connections",
				"graphItems": [
					{
						"name": "apache.conns_async_closing",
						"drawtype": "LINE"
					},
					{
						"name": "apache.conns_async_keep_alive",
						"drawtype": "LINE"
					},
					{
						"name": "apache.conns_async_writing",
						"drawtype": "LINE"
					},
					{
						"name": "apache.conns_total",
						"drawtype": "BOLD_LINE"
					}
				]
			},
			{
				"name": "Requests per second",
				"graphItems": [
					{
						"name": "apache.requests_rps",
						"drawtype": "GRADIENT_LINE"
					}
				]
			},
			{
				"name": "Workers",
				"graphType": "STACKED",
				"graphItems": [
					{
						"name": "apache.workers_busy",
						"drawtype": "LINE"
					},
					{
						"name": "apache.workers_idle",
						"drawtype": "LINE"
					}
				]
			}
		]
	},
	{
		"id": "apache.conns_async_keep_alive",
		"key": "apache.conns_async_keep_alive",
		"name": "Connections async keep alive",
		"type": "DEPENDENT_ITEM",
